public class BaseClass {
	public WebDriver driver;
	public ExcelDataProvider excel;
	public ConfigDataProvider config;
	public void setUpSuite()
	{
		
		 excel=new ExcelDataProvider();
		 cnfig=new ConfigDataProvider();
		
	}
	
	@BeforeClass
	public void setup(){
		driver=BrowserFactory.startApplication(driver, "Chrome", "https://www.globalsqa.com/angularJs-protractor/BankingProject/");
			
		
	}
	

}
